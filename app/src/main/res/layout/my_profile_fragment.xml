<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <providers.fairrepair.service.fairrepairpartner.helper.MyViewPager
        android:id="@+id/viewPagerVertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginBottom="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin">
        <include
            android:id="@+id/my_profile_one"
            layout="@layout/layout_profile_one"/>
        <include
            android:id="@+id/my_profile_two"
            layout="@layout/layout_profile_two"/>
    </providers.fairrepair.service.fairrepairpartner.helper.MyViewPager>
</LinearLayout>